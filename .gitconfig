[user]
	name = Julien Roland
	email = juroland@gmail.com
[url "https://juroland@github"]
	insteadOf = git://github
[push]
	default = matching
[http]
	sslverify = false
[core]
	autocrlf = input
	excludesfile = $HOME/.gitignore_global
[mergetool "fugitive"]
	cmd = vim -f -c \"Gdiff\" \"$MERGED\"
[merge]
	tool = fugitive
[alias]
    tree = log --graph --decorate --pretty=oneline --abbrev-commit --branches=*
